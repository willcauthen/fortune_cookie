<%= form_for @user do |f| %>
	<%= f.text_field :user_name, placeholder: "USERNAME", required: true %> <br>
	<%= f.text_field :email, placeholder: "emailzz", required: true %> <br>
	<%= f.password_field :password, placeholder: "teh secret codez", required: true %> <br>
	<%= f.password_field :password_confirmation, placeholder: "verify teh codez", required: true %> <br>
	<%= f.number_field :score %> <br>
	<%= f.submit  "save this user" %>
<% end %>